generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointments {
  id           String    @id @default(uuid())
  date         DateTime  
  time         String?
  patient      String
  madeby       String?
  department   String?
  services     String?
  note         String?
}
